<div class="row">
  {{#each pruebas}}
  <div class="col-md-3">
    <div class="card">
      <div class="card-body">
        
        <p>{{ materia }} </p>
        <p>{{ estado }} </p>
        <p>{{ preguntas }} / 20</p>
       
        <div class="preguntas">
          <a href="/notes/{{ _id }}" class="btn btn-primary btn-block">Realizar Prueba</a><br>
        </div>
       

        
      </div>
    </div>
  </div>
  
  {{else}}

  <div class="card mx-auto">
    <div class="card-body">
      <h1>Hola {{user.Nombre}}</h1>
      <p clsss="lead">Aun no hay pruebas</p>
    </div>
  </div>
  {{/each}}
</div>
